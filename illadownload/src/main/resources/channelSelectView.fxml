<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?scenebuilder-preview-i18n-resource channelSelectView_de.properties?>
<?scenebuilder-stylesheet applicationFrame.css?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="illarion.download.gui.controller.ChannelSelectionController">
    <children>
        <BorderPane fx:id="footer">
            <right>
                <Button alignment="CENTER_RIGHT" styleClass="button-green" text="%continue" />
            </right>
        </BorderPane>
        <BorderPane fx:id="content" prefHeight="200.0" prefWidth="200.0">
            <center>
                <Separator orientation="VERTICAL" prefHeight="200.0" />
            </center>
            <left>
                <VBox fillWidth="true">
                    <fx:define>
                        <ToggleGroup fx:id="storageMethodSelect" />
                    </fx:define>
                    <children>
                        <Label text="%descriptionGeneral" textAlignment="LEFT" wrapText="true">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                        </Label>
                        <Label text="%descriptionRelease" textAlignment="LEFT" wrapText="true" />
                        <Label text="%descriptionSnapshot" textAlignment="LEFT" wrapText="true" />
                    </children>
                    <padding>
                        <Insets right="10.0" />
                    </padding>
                </VBox>
            </left>
            <padding>
                <Insets bottom="10.0" top="20.0" />
            </padding>
            <right>
                <GridPane>
                    <children>
                        <ComboBox maxWidth="1.7976931348623157E308" minWidth="78.0" promptText="Veröffentlichung" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Release" />
                                    <String fx:value="Development" />
                                    <String fx:value="Veröffentlichung" />
                                    <String fx:value="Entwicklung" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                        <ComboBox maxWidth="1.7976931348623157E308" minWidth="78.0" prefWidth="-1.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Item 1" />
                                    <String fx:value="Item 2" />
                                    <String fx:value="Item 3" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                        <ComboBox maxWidth="1.7976931348623157E308" minWidth="78.0" prefWidth="-1.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Item 1" />
                                    <String fx:value="Item 2" />
                                    <String fx:value="Item 3" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                        <Label text="%labelClient" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                        <Label text="%labelEasyNPC" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                        <Label text="%labelEasyQuest" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                        <Label text="%labelMapEditor" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                        <ComboBox maxWidth="1.7976931348623157E308" minWidth="78.0" prefWidth="-1.0" promptText="test" GridPane.columnIndex="1" GridPane.rowIndex="0">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Item 1" />
                                    <String fx:value="Item 2" />
                                    <String fx:value="Item 3" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="2.0" prefWidth="76.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="198.0" minWidth="10.0" prefWidth="124.0" />
                    </columnConstraints>
                    <padding>
                        <Insets left="10.0" />
                    </padding>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                </GridPane>
            </right>
            <top>
                <Label style="-fx-font-size: 20pt" text="%title">
                    <BorderPane.margin>
                        <Insets bottom="10.0" />
                    </BorderPane.margin>
                </Label>
            </top>
        </BorderPane>
    </children>
</AnchorPane>
